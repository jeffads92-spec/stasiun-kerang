{
    "name": "jeffads92-spec/stasiun-kerang",
    "description": "Restaurant Management System - Complete POS and kitchen management solution",
    "type": "project",
    "keywords": [
        "restaurant",
        "pos",
        "management",
        "kitchen",
        "ordering",
        "payment"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "jeffads92-spec",
            "email": "info@stasiun-kerang.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "StasiunKerang\\": "src/"
        },
        "files": [
            "config/database.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "StasiunKerang\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=PSR12 api/",
        "phpcbf": "phpcbf --standard=PSR12 api/",
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
